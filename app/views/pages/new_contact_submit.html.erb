<div class = "container-fluid">
  <div class = "col-md-4">
    <table class = "table table-bordered table-hover">
      <caption>New Contact Info</caption>
      <tr>
        <th>First Name</th>
        <td><%= params[:first_name] %></td>
      </tr>
      <tr>
        <th>Last Name</th>
        <td><%= params[:last_name] %></td>
      </tr>
        <th>Email</th>
        <td><a href="mailto:<%= params[:email] %>"><%= params[:email] %></a></td>
      </tr>
      <tr>
        <th>Phone Number</th>
        <td><%= params[:phone] %></td>
      </tr>
    </table>
  </div>
</div>